
var arr = [1, 3, 3, 4,8,8,8,8];
var target = 6;
var result = [];
var done = {};

for (var i = 0; i < arr.length; i++) {
  var comp = target - arr[i];
  
  // Check if the complement exists in the `done` object
  if (done[comp] !== undefined) {
    result.push(arr[i]);
    result.push(comp);
  }

  // Add the current number to the `done` object
  done[arr[i]] = true;
}

console.log(done);
console.log(result);
