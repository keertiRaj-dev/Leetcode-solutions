var arr = [1, 5, 2, 3, 5];
var result = [];

for(var i=0; i<arr.length; i++){
    var flag = 0;
    for(var j=i+1; j<arr.length; j++){
        if(arr[j]>arr[i]){
            result.push(arr[j])
            flag++;
            break;
        }
    }
    if(flag==0){
        result.push(-1)
    }
}

console.log(result);
   
