//bubble sort

var arr =[1,5,4,6,8,3,2,0];

for(var i=0; i<arr.length-1; i++){
    var swapped = false;
    var temp;
    for(var j=0; j<arr.length-1-i; j++){
        if(arr[j]>arr[j+1]){
            temp = arr[j];
            arr[j]=arr[j+1];
            arr[j+1]=temp;
            swapped = true;
         
        }
       
    }
if(swapped===false){
            break;
}
        
}

console.log(arr)
